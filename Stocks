Sub stock()
    Const FIRST_DATA_ROW As Integer = 2
    

Dim Ticker As String
Dim InputRow As Integer
Dim OutPutCol As Integer
Dim LastRow As Long
Dim OutputRow As Integer

Dim OpenPrice As Double
Dim ClosePrice As Double
Dim YearChange As Double
Dim YearCol As Integer


LastRow = 7

'---LastRow = Cells(1, 1).End(x1Down).Row
OutPutCol = 9
OutputRow = 2


'---Output Ticker
For InputRow = FIRST_DATA_ROW To LastRow
    Ticker = Cells(FIRST_DATA_ROW, 1).Value
    OpenPrice = Cells(FIRST_DATA_ROW, 3).Value
    ClosePrice = Cells(FIRST_DATA_ROW, 6).Value
    YearChange = Cells(FIRST_DATA_ROW, 10).Value
    
    
        If InputRow <> OutPutCol Then
            Cells(OutputRow, OutPutCol) = Ticker
                OutputRow = OutputRow + 1
    End If
Next
    
'---Calculate YearlyChange




End Sub
